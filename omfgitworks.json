                                    "content": "import { createApp } from 'vue'\nimport App from './App.vue'\nimport router from './router'\nimport 'bootstrap/dist/css/bootstrap.css'\n\nconst app = createApp(App)\n\napp.use(router)\n\napp.mount('#app')\n"
